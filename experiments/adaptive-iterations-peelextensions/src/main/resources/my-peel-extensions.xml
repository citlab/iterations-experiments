<?xml version="1.0" encoding="UTF-8"?>

<beans xmlns="http://www.springframework.org/schema/beans"
       xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
       xmlns:context="http://www.springframework.org/schema/context"
       xsi:schemaLocation="http://www.springframework.org/schema/beans http://www.springframework.org/schema/beans/spring-beans.xsd
                           http://www.springframework.org/schema/context http://www.springframework.org/schema/context/spring-context-3.0.xsd">

    <context:annotation-config/>

    <!-- Dstat -->
    <bean id="dstat-0.7.2" class="de.tuberlin.cit.experiments.iterations.beans.system.Dstat" parent="system">
        <constructor-arg name="version" value="0.7.2"/>
        <constructor-arg name="configKey" value="dstat"/>
        <constructor-arg name="lifespan" value="RUN"/>
    </bean>

</beans>
